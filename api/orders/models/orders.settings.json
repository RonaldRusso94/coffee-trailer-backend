{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "name": "orders",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "number": {
      "type": "string"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "ordering",
        "preparing",
        "delivering",
        "completed"
      ],
      "default": "ordering"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "orders"
    },
    "order_items": {
      "via": "order",
      "collection": "order-item"
    }
  }
}
